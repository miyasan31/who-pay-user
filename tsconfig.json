{
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"baseUrl": "./",
		"paths": {
			"assets": ["./assets/*"],
			"src": ["./src/*"]
		}
	},
	"extends": "expo/tsconfig.base",
	"include": ["**/*.ts", "**/*.tsx", "**/*.js"],
	"exclude": ["node_modules", "out"]
}
